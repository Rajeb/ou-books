df = df.withColumn('tile', F.split('path', '/'))\
           .withColumn('tile', F.col('tile')[F.size('tile') -1])
    df = df.withColumn('tilexy', F.split('tile', '\.'))\
           .withColumn('tilexy', F.col('tilexy')[F.size('tilexy') -2]) 
    df = df.withColumn('tile_y', F.split('tilexy', '\_')).withColumn('tile_y', F.col('tile_y')[2]) 
    df = df.withColumn('tile_x', F.split('tilexy', '\_')).withColumn('tile_x', F.col('tile_x')[1]) 
